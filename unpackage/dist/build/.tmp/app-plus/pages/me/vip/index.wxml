<view class="content"><view class="vip-title">开通会员</view><block wx:for="{{vipList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="vip-warp"><view class="vip-warp-title">{{item.title}}</view><view class="vip-warp-list"><view class="vip-warp-item">{{"原价：￥"+item.originalPrice}}</view><view class="vip-warp-item">{{"现价：￥"+item.presentPrice}}</view><view class="vip-warp-item"><button class="btn" type="primary" size="mini" plain="{{true}}" data-event-opts="{{[['tap',[['handleBtn',['$event']]]]]}}" bindtap="__e">确认开通</button></view></view><view class="vip-warp-desc">{{item.desc}}</view></view></block></view>