<view class="contanner"><view class="img-wrapper"><image class="image" src="/static/banner.jpg" lazy-load="{{true}}" mode="widthFix"></image></view><view class="bar-wrapper"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['handleCheckLabel',[index]]]]]}}" class="{{['bar-item',isTitleBar==index?'bar-item-active':'']}}" bindtap="__e">{{item.label}}</view></block></view><view class="content"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]],['reset',[['formReset',['$event']]]]]}}" bindsubmit="__e" bindreset="__e"><view class="uni-form-item uni-column"><view class="form-lable">账号：</view><view class="form-inpput"><input placeholder-class="placeholder" type="text" placeholder="请输入电话号码或者ID号" data-event-opts="{{[['input',[['__set_model',['$0','userName','$event',[]],['userInfo']]]]]}}" value="{{userInfo.userName}}" bindinput="__e"/></view></view><view class="uni-form-item uni-column"><view class="form-lable">密码：</view><view class="form-inpput"><input type="password" placeholder-class="placeholder" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['userInfo']]]]]}}" value="{{userInfo.password}}" bindinput="__e"/></view></view><view class="uni-btn-v"><button type="primary" loading="{{loading}}" form-type="submit">登录</button><view class="tips">还没有账号？<router-link vue-id="1" to="/pages/register/index" bind:__l="__l" vue-slots="{{['default']}}">现在注册</router-link></view></view></form></view></view>